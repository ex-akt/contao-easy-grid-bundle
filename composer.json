{
    "name": "ex-akt/contao-easy-grid-bundle",
    "type": "symfony-bundle",
    "description": "Easy grid for Contao",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Samuel Heer",
            "homepage": "https://github.com/samuelheer"
        }
    ],
    "require": {
        "php": "^7.0",
        "contao/core-bundle": "4.9.*",
        "erdmannfreunde/euf_grid": "^3.1"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "exakt\\EasyGridBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "exakt\\EasyGridBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "exakt\\EasyGridBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/ex-akt/contao-easy-grid-bundle/issues",
        "source": "https://github.com/ex-akt/contao-easy-grid-bundle/contao-easy-grid-bundle"
    }
}
